{
	"id": "claw-browser-automation",
	"name": "Browser Automation",
	"description": "Reliable browser automation layer for OpenClaw using Playwright",
	"configSchema": {
		"type": "object",
		"properties": {
			"maxContexts": { "type": "number" },
			"headless": { "type": "boolean" },
			"dbPath": { "type": "string" },
			"artifactsDir": { "type": "string" },
			"artifactsMaxSessions": { "type": "number" },
			"redactSensitiveActionInput": { "type": "boolean" },
			"sensitiveActionInputKeys": { "type": "array", "items": { "type": "string" } },
			"redactTypedActionText": { "type": "boolean" },
			"autoApprove": { "type": "boolean" },
			"sitePlugins": {
				"type": "array",
				"items": {
					"type": "object",
					"properties": {
						"module": { "type": "string" },
						"enabled": { "type": "boolean" },
						"options": { "type": "object" }
					},
					"required": ["module"],
					"additionalProperties": false
				}
			},
			"logLevel": { "type": "string" }
		},
		"additionalProperties": false
	}
}
